@model List<Partner>
<div class="client-section ptb-100">
    <div class="container">
        <!--clients logo start-->
        <div class="row align-items-center">
            <div class="col-md-12">
                <div class="owl-carousel owl-theme clients-carousel dot-indicator">
                    @foreach (var partnr in Model)
                    {
                        <div class="item single-client">
                            <img src="~/uploads/@partnr.Photo" alt="client logo" class="client-img">
                        </div>
                    }
                </div>
            </div>
        </div>
        <!--clients logo end-->
    </div>
</div>
